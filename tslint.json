{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "semicolon":true,
    "trailing-comma": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "array-type": false
  }
}